{
  "$schema": "https://swc.rs/schema.json",
  "minify": true,
  "jsc": {
    "minify": {
      "compress": {
        "arrows": true,
        "booleans": true,
        "collapse_vars": true,
        "comparisons": true,
        "computed_props": true,
        "conditionals": true,
        "dead_code": true,
        "directives": true,
        "drop_console": true,
        "drop_debugger": true,
        "evaluate": true,
        "if_return": true,
        "inline": 3,
        "join_vars": true,
        "loops": true,
        "negate_iife": true,
        "properties": true,
        "reduce_funcs": true,
        "reduce_vars": true,
        "sequences": true,
        "side_effects": true,
        "switches": true,
        "typeofs": true,
        "unused": true
      },
      "mangle": true,
      "format": {
        "comments": false
      }
    }
  }
}
